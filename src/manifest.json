{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.3/MicrosoftTeams.schema.json",
    "manifestVersion": "1.3",
    "version": "0.0.1",
    "id": "743fe636-3405-4f09-bc8a-96d12883db40",
    "packageName": "com.poly.bot",
    "developer": {
        "name": "Poly",
        "websiteUrl": "https://www.poly.com",
        "privacyUrl": "https://www.poly.com/privacy",
        "termsOfUseUrl": "https://www.poly.com/terms"
    },
    "name": {
        "short": "Poly Bot",
        "full": "I can help you dial calls, search the Poly knowledge base, or talk with a Poly representative"
    },
    "description": {
        "short": "Dial calls, search the Poly knowledge base, or talk with a Poly representative",
