            ],
            "commandLists": [
                {
                    "scopes": [
                        "team",
                        "personal"
                    ],
                    "commands": [
                        {
                            "title": "Chat",
                            "description": "Chat with a poly representative"
                        },
                        {
                            "title": "Dial",
                            "description": "Dial [dial string] via Poly RealPresence Desktop/Mobile"
                        },
                        {
                            "title": "Search",
                            "description": "Search the Poly knowledgebase"
                        }
                    ]
                }
            ]
        }
    ],
    "permissions": [],
    "validDomains": []
