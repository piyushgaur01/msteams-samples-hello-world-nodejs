{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.3/MicrosoftTeams.schema.json",
    "manifestVersion": "1.3",
    "version": "1.0.0",
    "id": "743fe636-3405-4f09-bc8a-96d12883db40",
    "packageName": "com.poly.bot",
    "developer": {
        "name": "Piyush Gaur",
        "websiteUrl": "https://www.poly.com",
        "privacyUrl": "https://www.poly.com/privacy",
        "termsOfUseUrl": "https://www.poly.com/about"
    },
    "name": {
        "short": "Poly Bot",
        "full": "I can help you join meetings."
    },
    "description": {
        "short": "I can help you join meetings.",
        "full": "I can help you join meetings, connect with a Poly representatives and search knowledge base for FAQs."
    },
    "icons": {
        "outline": "poly20x20.png",
        "color": "poly96x96.png"
    },
    "accentColor": "#ffffff",
    "configurableTabs": [
        {
            "configurationUrl": "https://poly-bots.herokuapp.com/configure",
            "canUpdateConfiguration": true,
            "scopes": [
                "team"
            ]
        }
    ],
    "bots": [
        {
            "botId": "743fe636-3405-4f09-bc8a-96d12883db40",
            "scopes": [
                "personal",
                "team"
            ],
            "commandLists": [
                {
                    "scopes": [
                        "team",
                        "personal"
                    ],
                    "commands": [
                        {
                            "title": "Chat",
                            "description": "Chat with a poly representative"
                        },
                        {
                            "title": "Dial",
                            "description": "Dial [Dial string or VMR number] a call using RP Mobile/Desktop client"
                        },
                        {
                            "title": "Support",
                            "description": "Search Poly knowledge base / FAQs"
                        }
                    ]
                }
            ]
        }
    ],
    "permissions": [],
    "validDomains": ["*.flaticon.com"]
}